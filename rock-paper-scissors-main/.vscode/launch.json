{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch index.html",
      "type": "firefox",
      "request": "launch",
      "reAttach": true,
      "url": "http://127.0.0.1:5173",
      "skipFiles": [
        "**/node_modules/**",
        "**/client**"
      ],
      "pathMappings": [
        {
          "url": "http://localhost:5173",
          "path": "${workspaceFolder}/src"
        },
        {
          "url": "http://localhost:5173",
          "path": "${workspaceFolder}"
        }
      ]
    }
  ]
}